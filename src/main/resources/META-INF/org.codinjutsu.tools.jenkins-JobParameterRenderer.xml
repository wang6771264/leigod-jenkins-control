                                order="first"
                                id="BUILT_IN_RENDERER" />
        <buildParameterRenderer implementation="org.codinjutsu.tools.jenkins.view.parameter.SeparatorRenderer" />
        <buildParameterRenderer implementation="org.codinjutsu.tools.jenkins.view.parameter.renderer.NodeParameterRenderer" />
        <buildParameterRenderer implementation="org.codinjutsu.tools.jenkins.view.parameter.renderer.HiddenParameterRenderer" />
        <buildParameterRenderer implementation="org.codinjutsu.tools.jenkins.view.parameter.renderer.PatchParameterRenderer" />
